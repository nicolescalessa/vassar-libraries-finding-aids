			<unittitle label="Title:" encodinganalog="245"> Lesbian, Gay, Bisexual and Transgender Lives Manuscript Collection</unittitle>
			<unitdate type="inclusive" label="Dates:" encodinganalog="245$f"> 1990-2002</unitdate>
			<physdesc label="Quantity:" encodinganalog="300$a">1.3 cubic feet (3 boxes)</physdesc>
			<repository label="Repository:" encodinganalog="852$a">
